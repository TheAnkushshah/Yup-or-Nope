{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "default",
  "rsc": true,
  "tsx": false,
  "icons": [
      {
        "src": "/logo.png",
        "type": "image/png",
        "sizes": "400x400"
      },
      {
        "src": "/apple-touch-icon.png",
